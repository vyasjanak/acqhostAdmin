<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.recon.entities.PayoutMaster" table="payout_master">
		<id name="id" type="java.lang.Long">
			<column name="id" />
			<generator class="native" />
		</id>
		<property name="conn_id" type="java.lang.String">
			<column name="conn_id" length="100" />
		</property>
		<property name="toll_plaza_id" type="java.lang.String">
			<column name="toll_plaza_id" length="100" />
		</property>
		<property name="txn_count" type="java.lang.String">
			<column name="txn_count" length="100" />
		</property>
		<property name="total_amount" type="java.lang.String">
			<column name="total_amount" length="100" />
		</property>
		<property name="net_amount" type="java.lang.String">
			<column name="net_amount" length="100" />
		</property>
		<property name="hdfc_amount" type="java.lang.String">
			<column name="hdfc_amount" length="100" />
		</property>
		<property name="acq_amount" type="java.lang.String">
			<column name="acq_amount" length="100" />
		</property>
		<property name="to_date" type="java.lang.String">
			<column name="to_date" length="100" />
		</property>
		<property name="from_date" type="java.lang.String">
			<column name="from_date" length="100" />
		</property>
		
	</class>
</hibernate-mapping>